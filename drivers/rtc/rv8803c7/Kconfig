# RV_8803_C7 driver configuration

# Copyright (c) 2024 CATIE
# SPDX-License-Identifier: Apache-2.0

menuconfig RTC_RV8803C7
    bool "RTC RV-8803-C7 peripheral"
    default y
    depends on DT_HAS_MICROCRYSTAL_RV_8803_C7_ENABLED
    select I2C
    help
      Enable driver for MICRO CRYSTAL RV-8803-C7 smt real-time clock module on i2c-bus
